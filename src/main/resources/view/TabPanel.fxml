<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.StackPane?>

<TabPane style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab fx:id="taskTab">
            <content>
                <StackPane fx:id="taskListPanelPlaceholder" styleClass="pane-with-border" />
            </content>
        </Tab>
        <Tab fx:id="supplierTab">
            <content>
                <StackPane fx:id="supplierListPanelPlaceholder" />
            </content>
        </Tab>
        <Tab fx:id="orderTab">
            <content>
                <StackPane fx:id="orderListPanelPlaceholder" />
            </content>
        </Tab>
    </tabs>
</TabPane>